        <script type="text/javascript">
            var timer_id;
            function scroll_iframe(frm, inc, dir) {
                if (timer_id)
                    clearTimeout(timer_id);
                if (window.frames[frm]) {
                    if (dir == "v")
                        window.frames[frm].scrollBy(0, inc);
                    else
                        window.frames[frm].scrollBy(inc, 0);
                    timer_id = setTimeout("scroll_iframe('" + frm + "'," + inc + ",'" + dir + "')", 20);
                }
            }

            function stopScroll() {
                if (timer_id)
                    clearTimeout(timer_id);
            }
        </script>		
        <a href="#" class="mov_esquerda f_left" onMouseOver="this.style.color = 'red';
                scroll_iframe('scr1', -4, 'h');
                window.status = 'Left.';
                return true" onMouseOut="this.style.color = '';
                        stopScroll();
                        window.status = '';
                        return true"></a>
        <div id="area_noticias" class="f_left"><iframe name=scr1 src="pastor_home.php" scrolling="no" frameborder="0" allowtransparency="1" width="422" height="152"></iframe></div>
        <a href="#" class="mov_direita f_left" onMouseOver="this.style.color = 'red';
                scroll_iframe('scr1', 4, 'h');
                window.status = 'right.';
                return true" onMouseOut="this.style.color = '';
                        stopScroll();
                        window.status = '';
                        return true"></a>
