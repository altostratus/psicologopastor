    <div id="area_formulario">    <script type="text/javascript" language="javascript">
	function retVal(){
		var f = document.cad;
		if(f.nome.value == ""){
			f.nome.value = "Nome:";
		} 
		
		if(f.celular_ddd.value == ""){
			f.celular_ddd.value = "DDD";
		}
		
		if(f.celular.value == ""){
			f.celular.value = "Celular:";
		}
		
		if(f.fone_ddd.value == ""){
			f.fone_ddd.value = "DDD";
		}
		
		if(f.fone.value == ""){
			f.fone.value = "Telefone Residencial:";
		}
		
			if(f.cpf.value == ""){
				f.cpf.value = "CPF / CNPJ:";
			}
		
		if(f.email.value == ""){
			f.email.value = "Email:";
		}
		
		if(f.rua.value == ""){
			f.rua.value = "Rua:";
		}
		
		if(f.numero.value == ""){
			f.numero.value = "Número:";
		}
		
		if(f.bairro.value == ""){
			f.bairro.value = "Bairro:";
		}
		
		if(f.cidade.value == ""){
			f.cidade.value = "Cidade:";
		}
		
		if(f.cep.value == ""){
			f.cep.value = "CEP:";
		}
		
		if(f.msg.value == ""){
			f.msg.value = "Em caso de dúvida ou sugestão deixe sua mensagem:";
		}
		
		if(f.mais_curso.value == ""){
			f.mais_curso.value = "Digite aqui o seu curso desejado:";
		}
	}
	
	function limpa(campo) {
		var f = document.cad;
		
		switch (campo) {
			case 'nome' :
				if(f.nome.value == "Nome:"){
					f.nome.value = "";
				} 
				break;	
				
			case 'celular_ddd' :
				if(f.celular_ddd.value == "DDD"){
					f.celular_ddd.value = "";
				}
				break;	
				
			case 'celular' :
				if(f.celular.value == "Celular:"){
					f.celular.value = "";
				}
				break;	
				
			case 'fone_ddd' :
				if(f.fone_ddd.value == "DDD"){
					f.fone_ddd.value = "";
				}
				break;	
				
			case 'fone' :
				if(f.fone.value == "Telefone Residencial:"){
					f.fone.value = "";
				}
				break;	
				
			case 'email' :
				if(f.email.value == "Email:"){
					f.email.value = "";
				}		
				break;
		
			case 'cpf' :
				if(f.cpf.value == "CPF / CNPJ:"){
					f.cpf.value = "";
				}	
				break;	
			
			case 'rua' :
				if(f.rua.value == "Rua:"){
					f.rua.value = "";
				}	
				break;		

			case 'numero' :
				if(f.numero.value == "Número:"){
					f.numero.value = "";
				}	
				break;	
				
			case 'bairro' :
				if(f.bairro.value == "Bairro:"){
					f.bairro.value = "";
				}		
				break;		

			case 'cidade' :
				if(f.cidade.value == "Cidade:"){
					f.cidade.value = "";
				}	
				break;	
				
			case 'cep' :
				if(f.cep.value == "CEP:"){
					f.cep.value = "";
				}		
				break;		

			case 'msg' :
				if(f.msg.value == "Em caso de dúvida ou sugestão deixe sua mensagem:"){
					f.msg.value = "";
				}		
				break;	

			case 'mais_curso' :
				if(f.mais_curso.value == "Digite aqui o seu curso desejado:"){
					f.mais_curso.value = "";
				}		
				break;		
		}
	}
</script>
