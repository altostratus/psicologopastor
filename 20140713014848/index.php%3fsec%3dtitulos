        <script type="text/javascript">

            function limpa_topo(campo) {
                var f = document.getElementById(campo);

                switch (campo) {
                    case 'busca_top_field' :
                        if (f.value.toUpperCase() == "BUSCAR NO SITE") {
                            f.value = "";
                        }
                        break;
                    case 'formNewsEmailTopo' :
                        if (f.value.toUpperCase() == "E-MAIL") {
                            f.value = "";
                        }
                        break;
                }
            }

            function retVal_topo(campo) {
                var f = document.getElementById(campo);
                if ((f.value == "")) {
                    if (campo == 'busca_top_field') {
                        f.value = "BUSCAR NO SITE";
                    } else {
                        f.value = "E-MAIL";
                    }
                }
            }

            function CapEnterBusca(event) {
                if (event.keyCode == 13) {
                    valida_busca();
                }
            }
            function valida_busca() {
                f = document.getElementById('busca_top_field');
                if ((f.value == '') || (f.value.toUpperCase() == 'BUSCAR NO SITE')) {
                    alert('Informe o que voce deseja buscar no site!');
                    f.focus();
                    return false;
                } else {
                    location.href = '/index.php?sec=busca&q=' + f.value;
                    return true;
                }
            }

            function valida_email() {
                f = document.getElementById('newsletter_top_field');
                if ((f.value == '') || (f.value.toUpperCase() == 'E-MAIL')) {
                    alert('Informe seu e-mail!');
                    f.focus();
                    return false;
                } else {
                    location.href = '/index.php?sec=grava_email&email=' + f.value;
                    return true;
                }
            }
        </script>
