<script type="text/javascript">
	
	function limpa_topo_not(campo) {
		var f = document.getElementById(campo);
                    if(f.value.toUpperCase() == "BUSCAR NOTICIA"){
			f.value = "";
		}
	}		
	
	function retVal_topo_not(campo){
		var f = document.getElementById(campo);
		if((f.value == "")){
			f.value = "BUSCAR NOTICIA";
		} 
	}
	
	function CapEnterNot(event) { if(event.keyCode==13 ) { valida_busca_not(); } }
	function valida_busca_not(){
		f = document.getElementById('busca_not_field');
		if((f.value == '') || (f.value.toUpperCase() == 'BUSCAR NOTICIA')){
			alert('Informe que not&iacute;cia voc&ecirc; deseja buscar no site!');
			f.focus();
			return false;
		}	else{
			location.href = '/index.php?sec=noticias_busca&q='+f.value;
			return true;
		}
	}
</script>
