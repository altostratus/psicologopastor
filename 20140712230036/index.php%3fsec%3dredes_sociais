			<script type="text/javascript">
				function retVal(){
					var f = document.cad;
					if(f.nome.value == ""){
						f.nome.value = "Nome:";
					} 
					
					if(f.pai.value == ""){
						f.pai.value = "Nome do pai:";
					}

					if(f.mae.value == ""){
						f.mae.value = "Nome da mãe:";
					}

					if(f.data_nascimento.value == ""){
						f.data_nascimento.value = "Data de Nascimento";
					}
					 
					if(f.email.value == ""){
						f.email.value = "E-mail:";
					}
					 		
					if(f.profissao.value == ""){
						f.profissao.value = "Profissão:";
					}
						
					if(f.fone_ddd.value == ""){
						f.fone_ddd.value = "DDD";
					}
						
					if(f.fone.value == ""){
						f.fone.value = "Telefone Residencial:";
					}
						
					if(f.escolaridade.value == ""){
						f.escolaridade.value = "Escolaridade:";
					}
					
					if(f.dddt.value == ""){
						f.dddt.value = "DDD";
					}
						
					if(f.fonet.value == ""){
						f.fonet.value = "Telefone Comercial:";
					}
						
					if(f.rua.value == ""){
						f.rua.value = "Rua:";
					}
					
					if(f.numero.value == ""){
						f.numero.value = "Número:";
					}
						
					if(f.celular_ddd.value == ""){
						f.celular_ddd.value = "DDD";
					}
						
					if(f.celular.value == ""){
						f.celular.value = "Telefone Celular:";
					}
							
					if(f.bairro.value == ""){
						f.bairro.value = "Bairro:";
					}		

					if(f.comple.value == ""){
						f.comple.value = "Complemento:";
					}	

					if(f.igreja.value == ""){
						f.igreja.value = "Igreja";
					}	

					if(f.cidade.value == ""){
						f.cidade.value = "Cidade:";
					}	

					if(f.cep.value == ""){
						f.cep.value = "CEP";
					}		
					
					if(f.rg.value == ""){
						f.rg.value = "RG:";
					}		

					if(f.cpf.value == ""){
						f.cpf.value = "CPF:";
					}		
					
					if(f.naturalidade.value == ""){
						f.naturalidade.value = "Naturalidade:";
					}	
				}
				
				function limpa(campo) {
					var f = document.cad;
					
					switch (campo) {
						case 'nome' :
							if(f.nome.value == "Nome:"){
								f.nome.value = "";
							} 
							break;	
							
						case 'pai' :
							if(f.pai.value == "Nome do pai:"){
								f.pai.value = "";
							}
							break;	
							
						case 'mae' :
							if(f.mae.value == "Nome da mãe:"){
								f.mae.value = "";
							} 
							break;	
							
						case 'data_nascimento' :
							if(f.data_nascimento.value == "Data de Nascimento"){
								f.data_nascimento.value = "";
							} 
							break;	
	
						case 'email' :
							if(f.email.value == "E-mail:"){
								f.email.value = "";
							} 		
							break;
	
						case 'profissao' :
							if(f.profissao.value == "Profissão:"){
								f.profissao.value = "";
							}	
							break;	
							
						case 'fone_ddd' :
							if(f.fone_ddd.value == "DDD"){
								f.fone_ddd.value = "";
							}	
							break;
							
						case 'fone' :
							if(f.fone.value == "Telefone Residencial:"){
								f.fone.value = "";
							}	
							break;	
	
						case 'escolaridade' :
							if(f.escolaridade.value == "Escolaridade:"){
								f.escolaridade.value = "";
							}	
							break;	
							
						case 'dddt' :
							if(f.dddt.value == "DDD"){
								f.dddt.value = "";
							}	
							break;
							
						case 'fonet' :
							if(f.fonet.value == "Telefone Comercial:"){
								f.fonet.value = "";
							}	
							break;
	
						case 'rua' :
							if(f.rua.value == "Rua:"){
								f.rua.value = "";
							}
							break;		
	
						case 'numero' :
							if(f.numero.value == "Número:"){
								f.numero.value = "";
							}
							break;		
							
						case 'celular_ddd' :
							if(f.celular_ddd.value == "DDD"){
								f.celular_ddd.value = "";
							}	
							break;
							
						case 'celular' :
							if(f.celular.value == "Telefone Celular:"){
								f.celular.value = "";
							}		
							break;						
									
						case 'bairro' :
							if(f.bairro.value == "Bairro:"){
								f.bairro.value = "";
							}		
							break;	
	
						case 'comple' :
							if(f.comple.value == "Complemento:"){
								f.comple.value = "";
							}	
							break;			
	
						case 'igreja' :
							if(f.igreja.value == "Igreja"){
								f.igreja.value = "";
							}	
							break;	
	
						case 'cidade' :
							if(f.cidade.value == "Cidade:"){
								f.cidade.value = "";
							}	
							break;		
	
						case 'cep' :
							if(f.cep.value == "CEP"){
								f.cep.value = "";
							}		
							break;		
							
						case 'rg' :
							if(f.rg.value == "RG:"){
								f.rg.value = "";
							}		
							break;	
	
						case 'cpf' :
							if(f.cpf.value == "CPF:"){
								f.cpf.value = "";
							}		
							break;	
								
						case 'naturalidade' :
							if(f.naturalidade.value == "Naturalidade:"){
								f.naturalidade.value = "";
							}		
							break;	
					}
				}			
