				<script>
			function result_enquete(url) {
				window.open(url,'nova','width=256,height=307,resizable=0,status=0,menubar=0,scrollbars=0');
			}
			function IsEmpty() {
				var frm=document.enquete;
				var opcao = 0;
				for (var i = 0; i < frm.opcao.length; i++) {
					if (frm.opcao[i].checked) {
						opcao = frm.opcao[i].value;
					}
				}
				if (opcao == "0"){
					alert("Por favor, selecione uma alternativa!!");
				} else {
					var url = 'vota_enquete.php?codigo=' + opcao;
					result_enquete(url);
				}
			}
		</script>
