<script language="JavaScript" type="text/JavaScript">
  function limpa() {
    if (cad.nome.value=='Nome') {
      cad.nome.value='';
    }
  }

  function volta() {
    if (cad.nome.value=='') {
      cad.nome.value='Nome';
    }
  }
  
  function limpar() {
    if (cad.fone.value=='(DDD)-Fone') {
      cad.fone.value='';
    }
  }

  function voltar() {
    if (cad.fone.value=='') {
      cad.fone.value='(DDD)-Fone';
    }
  }
  
  function limp() {
    if (cad.rua.value=='Rua') {
      cad.rua.value='';
    }
  }

  function volt() {
    if (cad.rua.value=='') {
      cad.rua.value='Rua';
    }
  }

  function lim() {
    if (cad.numero.value=='Número') {
      cad.numero.value='';
    }
  }

  function vol() {
    if (cad.numero.value=='') {
      cad.numero.value='Número';
    }
  }

  function limpa_endereco() {
    if (cad.cidade.value=='Cidade') {
      cad.cidade.value='';
    }
  }

  function volta_endereco() {
    if (cad.cidade.value=='') {
      cad.cidade.value='Cidade';
    }
  }
  
  function limpa_cargo() {
    if (cad.pais.value=='País') {
      cad.pais.value='';
    }
  }

  function volta_cargo() {
    if (cad.pais.value=='') {
      cad.pais.value='País';
    }
  }

  function limpa_igreja() {
    if (cad.estado.value=='Estado') {
      cad.estado.value='';
    }
  }

  function volta_igreja() {
    if (cad.estado.value=='') {
      cad.estado.value='Estado';
    }
  }

  function limpa_cel() {
    if (cad.bairro.value=='Bairro') {
      cad.bairro.value='';
    }
  }

  function volta_cel() {
    if (cad.bairro.value=='') {
      cad.bairro.value='Bairro';
    }
  }

  function limpa_cidade() {
    if (cad.cep.value=='CEP') {
      cad.cep.value='';
    }
  }

  function volta_cidade() {
    if (cad.cep.value=='') {
      cad.cep.value='CEP';
    }
  }

  function limpa_1() {
    if (cad.msn.value=='Msn') {
      cad.msn.value='';
    }
  }

  function volta_1() {
    if (cad.msn.value=='') {
      cad.msn.value='Msn';
    }
  }

  function limpa_2() {
    if (cad.igreja.value=='Igreja') {
      cad.igreja.value='';
    }
  }

  function volta_2() {
    if (cad.igreja.value=='') {
      cad.igreja.value='Igreja';
    }
  }

  function limpa_3() {
    if (cad.cargo.value=='Cargo na Igreja') {
      cad.cargo.value='';
    }
  }

  function volta_3() {
    if (cad.cargo.value=='') {
      cad.cargo.value='Cargo na Igreja';
    }
  }
  
  function limpa_msg() {
    if (cad.mensagem.value=='Mensagem') {
      cad.mensagem.value='';
    }
  }

  function volta_msg() {
    if (cad.mensagem.value=='') {
      cad.mensagem.value='Mensagem';
    }
  }  
  
  function valida_dados(cad){
  	if(cad.nome.value=='Nome'){
		alert('Preencha o campo Nome do formulário para prosseguir.');
		cad.nome.focus();
		return (false);
	}
  	if(cad.fone.value=='(DDD)-Fone'){
		alert('Preencha o campo (DDD)-Fone do formulário para prosseguir.');
		cad.fone.focus();
		return (false);
	}
  	if(cad.rua.value=='Rua'){
		alert('Preencha o campo Rua do formulário para prosseguir.');
		cad.rua.focus();
		return (false);
	}
  	if(cad.numero.value=='Número'){
		alert('Preencha o campo Número do formulário para prosseguir.');
		cad.numero.focus();
		return (false);
	}
  	if(cad.bairro.value=='Bairro'){
		alert('Preencha o campo Bairro do formulário para prosseguir.');
		cad.bairro.focus();
		return (false);
	}
  	if(cad.cidade.value=='Cidade'){
		alert('Preencha o campo Cidade do formulário para prosseguir.');
		cad.cidade.focus();
		return (false);
	}
  	if(cad.cep.value=='CEP'){
		alert('Preencha o campo CEP do formulário para prosseguir.');
		cad.cep.focus();
		return (false);
	}
  	if(cad.estado.value=='Estado'){
		alert('Preencha o campo Estado do formulário para prosseguir.');
		cad.estado.focus();
		return (false);
	}
  	if(cad.pais.value=='País'){
		alert('Preencha o campo País do formulário para prosseguir.');
		cad.pais.focus();
		return (false);
	}
  	if(cad.msn.value=='Msn'){
		alert('Preencha o campo Msn do formulário para prosseguir.');
		cad.msn.focus();
		return (false);
	}
	return(true);
  }
</script>
